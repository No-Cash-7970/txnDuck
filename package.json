    "preprod": "yarn build",
    "prod": "next start",
    "pretest": "yarn compile-locales",
    "test": "jest --watch",
    "pretest:ci": "yarn compile-locales",
    "test:ci": "jest --ci",
    "pretest:e2e": "yarn compile-locales",
    "test:e2e": "playwright test",
    "install:dev": "yarn install && lefthook install && gulp buildJestChance"
  },
  "dependencies": {
    "@algorandfoundation/algokit-utils": "^2.3.1",
    "@tabler/icons-react": "^2.26.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "accept-language": "^3.0.18",
    "algosdk": "^2.4.0",
    "autoprefixer": "10.4.14",
    "daisyui": "^3.2.1",
    "gulp": "^4.0.2",
    "gulp-execa": "^5.0.0",
    "gulp-rename": "^2.0.0",
    "i18next": "^23.2.11",
    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-resources-to-backend": "^1.1.4",
    "jotai": "^2.2.2",
    "js-yaml": "^4.1.0",
    "next": "13.4.10",
    "postcss": "8.4.26",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.0.2",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@playwright/test": "^1.36.1",
    "@swc-jotai/react-refresh": "^0.0.8",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "chance": "^1.1.11",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "eslint-plugin-testing-library": "^5.11.0",
    "eslint-plugin-yml": "^1.8.0",
    "jest": "^29.6.1",
    "jest-chance": "https://github.com/No-Cash-7970/jest-chance",
    "jest-environment-jsdom": "^29.6.1",
    "lefthook": "^1.4.6",
    "ts-jest": "^29.1.1",
    "yaml-eslint-parser": "^1.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
